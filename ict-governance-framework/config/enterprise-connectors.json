{
  "connectors": {
    "azure-ad": {
      "name": "Azure Active Directory",
      "type": "identity",
      "priority": "critical",
      "enabled": true,
      "description": "Enterprise identity and access management integration",
      "capabilities": [
        "user-management",
        "group-management",
        "authentication",
        "authorization",
        "conditional-access"
      ],
      "endpoints": {
        "users": "/v1.0/users",
        "groups": "/v1.0/groups",
        "applications": "/v1.0/applications",
        "policies": "/v1.0/policies/conditionalAccessPolicies"
      },
      "rateLimit": {
        "requestsPerMinute": 600,
        "burstLimit": 100
      },
      "retryPolicy": {
        "maxAttempts": 3,
        "backoffMultiplier": 2,
        "initialDelay": 1000
      }
    },
    "defender-cloud-apps": {
      "name": "Microsoft Defender for Cloud Apps",
      "type": "security",
      "priority": "critical",
      "enabled": true,
      "description": "Cloud application security monitoring and governance",
      "capabilities": [
        "app-discovery",
        "security-alerts",
        "policy-enforcement",
        "activity-monitoring",
        "threat-detection"
      ],
      "endpoints": {
        "discoveredApps": "/v1.0/security/cloudAppSecurityProfiles",
        "alerts": "/v1.0/security/alerts",
        "activities": "/v1.0/auditLogs/signIns",
        "policies": "/v1.0/policies/conditionalAccessPolicies"
      },
      "rateLimit": {
        "requestsPerMinute": 30,
        "burstLimit": 10
      },
      "retryPolicy": {
        "maxAttempts": 5,
        "backoffMultiplier": 2,
        "initialDelay": 2000
      }
    },
    "servicenow": {
      "name": "ServiceNow ITSM",
      "type": "itsm",
      "priority": "high",
      "enabled": true,
      "description": "IT Service Management and workflow automation",
      "capabilities": [
        "incident-management",
        "change-management",
        "cmdb-integration",
        "service-catalog",
        "workflow-automation"
      ],
      "endpoints": {
        "incidents": "/api/now/table/incident",
        "changeRequests": "/api/now/table/change_request",
        "cmdbItems": "/api/now/table/cmdb_ci",
        "serviceCatalog": "/api/now/table/sc_cat_item"
