      "rateLimit": {
        "requestsPerMinute": 120,
        "burstLimit": 50
      },
      "retryPolicy": {
        "maxAttempts": 3,
        "backoffMultiplier": 1.5,
        "initialDelay": 1000
      }
    },
    "power-bi": {
