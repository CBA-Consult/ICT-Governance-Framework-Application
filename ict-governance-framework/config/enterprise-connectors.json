  "globalSettings": {
    "defaultTimeout": 30000,
    "maxConcurrentConnections": 50,
    "healthCheckInterval": 300000,
    "metricsRetention": 2592000000,
    "enableCircuitBreaker": true,
    "circuitBreakerThreshold": 5,
    "circuitBreakerTimeout": 60000,
    "enableCaching": true,
    "defaultCacheTTL": 300,
    "enableMetrics": true,
    "enableLogging": true,
    "logLevel": "info"
  },
  "integrationPatterns": {
    "realtime": {
      "description": "Real-time synchronous integration",
      "timeout": 10000,
      "retryAttempts": 3,
      "caching": false
    },
    "batch": {
      "description": "Batch processing integration",
      "timeout": 300000,
      "retryAttempts": 2,
      "caching": true,
      "cacheTTL": 3600
    },
    "streaming": {
      "description": "Streaming data integration",
      "timeout": 60000,
      "retryAttempts": 5,
      "caching": false
    }
  },
  "securitySettings": {
    "encryptionRequired": true,
    "tlsVersion": "1.2",
    "certificateValidation": true,
    "tokenRotationInterval": 3600000,
    "auditLogging": true,
    "sensitiveDataMasking": true
  }
}